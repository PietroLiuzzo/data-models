{
  "name": "alpheios-data-models",
  "version": "1.1.6",
  "description": "Alpheios Data Models",
  "main": "dist/alpheios-data-models.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "standard --fix && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "standard --fix && node ./node_modules/alpheios-node-build/dist/scripts/build.js webpack all",
    "build-prod": "standard --fix && node ./node_modules/alpheios-node-build/dist/scripts/build.js webpack production",
    "build-dev": "standard --fix && node ./node_modules/alpheios-node-build/dist/scripts/build.js webpack development",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/data-models.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/data-models/issues"
  },
  "homepage": "https://github.com/alpheios-project/data-models#readme",
  "devDependencies": {
    "alpheios-node-build": "file:../node-build",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "intl-messageformat": "^2.1.0",
    "jest": "^22.4.3",
    "standard": "^11.0.1",
    "source-map-loader": "^0.2.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0"
  },
  "peerDependencies": {
    "intl-messageformat": "^2.1.0"
  },
  "jest": {
    "verbose": true
  },
  "dependencies": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    }
  },
  "standard": {
    "ignore": [
      "**/dist"
    ]
  }
}
